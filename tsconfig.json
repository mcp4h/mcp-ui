{
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"ES2020",
			"DOM"
		],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"rootDir": "src",
		"outDir": "dist",
		"declaration": true,
		"emitDeclarationOnly": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": [
		"src"
	]
}
